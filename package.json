{
  "name": "remindir",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "19.1.0"
  },
  "scripts": {
    "start": "npm start --prefix api",
    "dev": "concurrently --raw \"npm run api\" \"npm run react\"",
    "react": "cd ui && npm run start",
    "api": "cd api && npm run dev",
    "install:react": "npm install --prefix ui",
    "install:api": "npm install --prefix api",
    "build:react": "npm run build --prefix ui",
    "heroku-postbuild": "concurrently \"npm:install:*\"; concurrently \"npm:build:*\";"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WallaceAnthony-FS/fullstack-todo.git"
  },
